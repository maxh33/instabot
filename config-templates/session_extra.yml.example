# Extra session (for 4-5 session days)
# Can run at various times: 8am, 3pm, or 8pm
# Duration target: 30-45 minutes (shorter)
username: YOUR_USERNAME_HERE
device: YOUR_DEVICE_ID_HERE

# General tech/dev content
hashtag-likers-top: [tech, software, coding, programming, developer, webdevelopment, api]
blogger-post-likers: [github, gitlab, stackoverflow, hackernoon, freecodecamp]

# Lighter session
total-likes-limit: 25
total-follows-limit: 8
total-interactions-limit: 35
total-successful-interactions-limit: 25

# Human-like behavior
likes-count: 1-2
likes-percentage: 50-70
follow-percentage: 25-35
stories-count: 0-2
stories-percentage: 30-50
watch-video-time: 5-15
watch-photo-time: 2-4
carousel-count: 0-2
carousel-percentage: 40-60
interact-percentage: 50-65
speed-multiplier: 1.3

interactions-count: 3-6
skipped-list-limit: 6-10
